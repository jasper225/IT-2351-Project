CREATE TABLE generation5 (
id INT NOT NULL AUTO_INCREMENT,
name VARCHAR(255) NOT NULL,
type1 VARCHAR(255) NOT NULL,
type2 VARCHAR(255) NULL,
PRIMARY KEY (id)
);

